	<title>wep page</title>
</head>
<body>
	

	<script type="text/javascript">
		var x=10;
		var call=myfunction();
		document.write(call);
		function myfunction() {
			var y=20;
			document.write(x);
			document.write("<br>");
			document.write(y);
			document.write("<br>");
		}
	
	</script>

</body>
</html>

